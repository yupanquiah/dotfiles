  "schemes": [
    {
      "cursorColor": "#F5E0DC",
      "selectionBackground": "#545663",
      "background": "#23272E",
      "foreground": "#E5E8F1",
      "black": "#45475A",
      "red": "#FF7F7F",
      "green": "#70f364",
      "yellow": "#FDF98B",
      "blue": "#7f8BE9",
      "purple": "#FF63D5",
      "cyan": "#7BECDA",
      "white": "#FDFDFD",
      "brightBlack": "#585B70",
      "brightRed": "#FF6A6A",
      "brightGreen": "#75E882",
      "brightYellow": "#FFFFA1",
      "brightBlue": "#7D7FFF",
      "brightPurple": "#FF90E1",
      "brightCyan": "#74FFE8",
      "brightWhite": "#A6ADC8",
      "name": "yupanquidev"
    }
  ],
  "showTabsInTitlebar": true,
  "showTerminalTitleInTitlebar": true,
  "tabWidthMode": "compact",
  "useAcrylicInTabRow": true,
  "windowingBehavior": "useNew"
