{
  "active_pane_modifiers": {
    "border_size": 0.0,
    "inactive_opacity": 1.0
  },
  "bottom_dock_layout": "contained",
  "agent_font_size": null,
  "allow_rewrap": "in_comments",
  "auto_indent": true,
  "auto_indent_on_paste": true,
  "auto_install_extensions": {
    "html": true
  },
  "autosave": "off",
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "buffer_font_size": 12.5,
  "buffer_font_family": "Monaspace Neon",
  "buffer_font_features": {
    "calt": true,
    "ss01": true,
    "ss02": true,
    "ss03": true,
    "ss04": true,
    "ss05": true,
    "ss06": true,
    "ss07": true,
    "ss08": true,
    "ss09": true,
    "ss10": true,
    "liga": true
  },
  "cursor_blink": true,
  "cursor_shape": "bar",
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Dark"
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": true,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "activate_on_close": "history",
    "show_close_button": "hover",
    "show_diagnostics": "all"
  },
  "ensure_final_newline_on_save": true,
  "format_on_save": "on",
  "formatter": "language_server",
  "tab_size": 2
}
