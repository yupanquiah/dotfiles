{
  "breadcrumbs.enabled": false,
  "diffEditor.renderGutterMenu": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.colorDecorators": true,
  "editor.comments.insertSpace": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.find.cursorMoveOnType": true,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.folding": true,
  "editor.fontFamily": "'Monaspace Neon'",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  "editor.fontSize": 12.5,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.indentation": true,
  "editor.indentSize": "tabSize",
  "editor.hideCursorInOverviewRuler": true,
  "editor.insertSpaces": true,
  "editor.inlineSuggest.enabled": false,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.hideCursoInOverviewRuler": true,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollBeyondLastLine": true,
  "editor.stickyScroll.enabled": true,
  "editor.inlineSuggest.fontFamily": "'Monaspace Krypton'",
  "editor.suggestFontSize": 12,
  "editor.showFoldingControls": "never",
  "editor.renderWhitespace": "none",
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.codeLens": false,
  "editor.links": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "keyword.operator",
          "constant.numeric",
          "entity.name.selector",
          "entity.name.class",
          "entity.name.function",
          "entity.name.type"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
  "errorLens.fontFamily": "'Monaspace Krypton'",
  "errorLens.fontStyleItalic": true,
  "errorLens.fontSize": "12px",
  "errorLens.fontWeight": "400",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, pnpm-lock.yaml, .gitignore, astro.config.mjs, .gitattributes, .prettierrc, .prettierignore, .oxlintrc.json"
  },
  "extensions.ignoreRecommendations": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.decorations.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "go.toolsManagement.autoUpdate": true,
  "go.removeTags": {
    "tags": "2"
  },
  "html.format.extraLiners": "",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.quoteStyle": "single",
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "oneDarkPro.bold": true,
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  "oneDarkPro.vivid": true,
  "security.workspace.trust.untrustedFiles": "open",
  "scm.diffDecorations": "all",
  "scm.compactFolders": false,
  "scm.countBadge": "all",
  "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
  "terminal.integrated.fontFamily": "'MesloLGM NF'",
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.format.semicolons": "remove",
  "window.title": "${rootName}",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.showTabs": "single",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.indent": 8,
  "workbench.editor.decorations.badges": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "window.menuBarVisibility": "compact",
  "github.copilot.chat.agent.thinkingTool": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "remote.WSL.fileWatcher.polling": false,
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/vendor/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/vendor": true
  }
}
