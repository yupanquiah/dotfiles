      "brightCyan": "#00FFFF",
      "brightGreen": "#69FF94",
      "brightPurple": "#FF92DF",
      "brightRed": "#FF6E6E",
      "brightWhite": "#FFFFFF",
      "brightYellow": "#FFFF5C",
      "cursorColor": "#CFD1E4",
      "cyan": "#8BE9FD",
      "foreground": "#D3D3D3",
      "green": "#50FA7B",
      "name": "yupanquidev",
      "purple": "#FF79C6",
      "red": "#FF5555",
      "selectionBackground": "#1E2227",
      "white": "#F8F8F2",
      "yellow": "#F9FFA0"
